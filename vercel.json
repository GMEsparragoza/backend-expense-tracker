{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.js"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://backend-tu-dominio.com/api/$1"
        }
    ]
}